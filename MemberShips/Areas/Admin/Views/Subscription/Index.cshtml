@model IEnumerable<MemberShipsPro.Entities.Subscription>
@using MemberShipsPro.Areas.Admin.Models;
@{
    ViewBag.Title = "Subscriptions";
}

<h2>Subscriptions</h2>

<p>
    @Html.Partial("_CreateButtonPartial")
</p>
<table class="table table-striped table-condensed">
    <tr class="success">
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RegistrationCode)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RegistrationCode)
        </td>
       
           @Html.Partial("_TableButtonsPartial",
            new SmallButtonModel { Id = item.Id })
        
    </tr>
}

</table>
